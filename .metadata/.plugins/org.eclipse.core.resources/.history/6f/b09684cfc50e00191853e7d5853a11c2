
public class Vehicle {
	public String type;
	public int speed;
	
	public Vehicle(String type, int speed) {
		super();
		this.type = type;
		this.speed = speed;
	}
	
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	
	
}
