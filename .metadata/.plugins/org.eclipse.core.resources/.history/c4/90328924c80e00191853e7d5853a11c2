
public class Vehicle {
	private String type;
	private int speed;
	private int size;
	
	public Vehicle(int size, String direction) {
		super();
		this.size = size;
		this.direction = direction;
	}

	private String direction;
	
	public Vehicle(String type, int speed) {
		this.type = type;
		this.speed = speed;
	}
	
	public String getType() {
		return type;
	}
	
	public void setType(String type) {
		this.type = type;
	}
	
	public int getSpeed() {
		return speed;
	}
	
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	
	
}
