
public class Main {

	public static void main(String[] args) {
		Bank absaBank = new Bank("absa");
		
		String fourwaysBranchName = "Fourways";
		absaBank.addBranch("Fourways");
		absaBank.addCustomer("Fourways", "Lloyd", 10000);
		absaBank.addCustomer("Fourways", "Adam", 2122);
		absaBank.addCustomerTransaction("Fourways", "Lloyd", 500.43);
		absaBank.addCustomerTransaction("Fourways", "Adam", 53.33);
		
		String capeBranchName = "Cape Town";
		absaBank.addBranch(capeBranchName);
		absaBank.addCustomer(capeBranchName, "Gerald", 32);
		absaBank.addCustomerTransaction(capeBranchName, "Gerald", 45);
		absaBank.addCustomer(capeBranchName, customerName, initialAmount)
		
		
		absaBank.listCustomers("Fourways", false);
		absaBank.listCustomers("Cape Town", false);
	}

}
