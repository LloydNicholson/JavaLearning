
public class Resolution {
	private int height;
	private int width;
	
	public Resolution(int height, int width) {
		this.height = height;
		this.width = width;
	}
	
	public void changeScreenResolution(int height, int width) {
		System.out.println("New height is " + height + " New width is " + width);
	}
	
}
