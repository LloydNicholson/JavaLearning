
class Movie {
	private String name;
	
	public Movie(String name) {
		this.name = name;
	}
	
	public String plot() {
		return "No plot here";
	}
}

class Jaws extends Movie {
	public Jaws() {
		super("Jaws");
	}
	
	public String plot() {
		return "A shark eats lots of people";
	}
	
}

class IndependanceDay extends Movie {

	public IndependanceDay() {
		super("IndependanceDay");
		
	}
	
}


public class Main {

	public static void main(String[] args) {
		

	}
	
	

}
