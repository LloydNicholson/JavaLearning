
public class Room {
	private TV theTV;
	private Door theDoor;
	private Chair theChair;
	
	public Room(TV theTV, Door theDoor, Chair theChair) {
		this.theTV = theTV;
		this.theDoor = theDoor;
		this.theChair = theChair;
	}
	
	public void setTVRes(int i, int j) {
		theTV.getNativeResolution().changeScreenResolution(i, j);
	}
	
}
