
public class Dog extends Animal {

	private int eyes;
	private int legs;
	private int tail;
	private int teeth;
	private String coat;
	
	
	public Dog(String name, int size, int weight, int eyes, int legs, int tail, int teeth, String coat) {
		// Inheriting specific characteristics of parent class
		super(name, 1, 1, size, weight);
		
		// Defining specific attributes or fields of the Dog class
		this.eyes = eyes;
		this.legs = legs;
		this.tail = tail;
		this.teeth = teeth;
		this.coat = coat;
	}

}
